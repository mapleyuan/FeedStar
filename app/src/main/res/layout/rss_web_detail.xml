<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#eeeeee">

    <RelativeLayout
        android:id="@+id/socially_main_top_id"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:background="#ffffff"
        >

        <com.maple.yuanweinan.feedstar.view.SimpleImageView
            android:id="@+id/fs_back_id"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:background="@drawable/fs_back"
            />
    </RelativeLayout>
    <WebView
            android:id="@+id/webview_id"
            android:layout_below="@+id/socially_main_top_id"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
</RelativeLayout>